@{
    ViewBag.Title = "Contact";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

@model  List <_mvc_crud_app_1.DB_Context_EF.emp_regitration>
<table class="table table-hover table-dark">
    <tr>
        <th>id</th>
        <th>name</th>
        <th>email</th>
        <th>mobile</th>
        <th>city</th>
        <th>Edit </th>
        <th>Delete</th>

    </tr>

    @foreach (var item in Model)

    {
<tr>
    <td>@item.id </td>
    <td>@item.name</td>
    <td>@item.email</td>
    <td>@item.mobile</td>
    <td>@item.city</td>

    <td><a class="btn btn-success" href="@Url.Action("edit", "Home", new {item.id})">Edit</a></td>
    <td><a class="btn btn-danger" href="@Url.Action("delete", "Home", new {item.id})">Delete</a></td>

</tr>
    }
</table>












    </table>